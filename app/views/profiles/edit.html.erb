<
          <%= simple_form_for(@profile) do |f| %>
            <%= f.input :picture_cache, as: :hidden %>
            <%= f.input :picture %>
             <%= f.input :first_name %>
             <%= f.input :last_name %>
             <%= f.input :user_name %>
            <%= f.input :age %>
            <%= f.input :quote %>

            <%= f.submit "Update your profile"%>

          <% end %>
